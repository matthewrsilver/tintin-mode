#no tests for variable modification commands;

#cat                                #no || unterminated bare command;
#caT                                #no || unterminated bare command, cases folded;
#catzzzz;                           #no || command without space is not highlighted;
#cat;                               #no || terminated but incomplete command;
#cat x;                             #no || variable without value not highlighted;
#cat x data;                        #no || unbraced value;
#cat x {};                          #no || empty braced value;
#cat x {data};                      #no || braced value;
#cat {x} {data};                    #no || braced variable and value;
#cat {x}{data};                     #no || braced variable and value without spaces;
#cat {x} data;                      #no || braced variable and unbraced value;
#cat {x}data;                       #no || braced variable and unbraced value without spaces;
#cat{x}data;                        #no || braced variable without space is not highlighted;
#cat {} data;                       #no || good that the value isn't highlighted;
#cat x $result;                     #no || variable usage in value is correctly highlighted;
#cat {x$y} data;                    #no || variable usage in variable;
#if {$a} {#cat x t}{#cat x f};      #no || multiple definitions on same line;
#cat x a; #cat x b;                 #no || multiple definitions on same line;
#cat x; data;                       #no || prematurely terminated command not highlighted;